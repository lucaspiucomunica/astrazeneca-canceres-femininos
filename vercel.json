{
  "trailingSlash": false,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/assets/js/vendor/(.*)",
      "dest": "/assets/js/vendor/$1"
    },
    {
      "src": "/assets/css/vendor/(.*)",
      "dest": "/assets/css/vendor/$1"
    }
  ]
} 